@model SbdProjekto.Models.Rejon

@{
    ViewData["Title"] = "Create";
}

<h1>Tworzenie</h1>

<h4>Rejon</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Nazwa" class="control-label"></label>
                <input asp-for="Nazwa" class="form-control" />
                <span asp-validation-for="Nazwa" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Wojewodztwo" class="control-label"></label>
                <input asp-for="Wojewodztwo" class="form-control" />
                <span asp-validation-for="Wojewodztwo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <a class="btn btn-primary" asp-action="Index">Wróć do listy</a>
                <input type="submit" value="Dodaj" class="btn btn-success" />
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
