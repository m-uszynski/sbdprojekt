@model SbdProjekto.Models.Przesylka

@{
    ViewData["Title"] = "Szczegóły";
}

<h1>Szczegóły</h1>

<div>
    <h4>Przesyłka</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Waga)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Waga)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Szerokosc)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Szerokosc)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Wysokosc)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Wysokosc)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Dlugosc)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Dlugosc)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Magazyn)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Magazyn.Nazwa)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.RodzajPrzesylki)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.RodzajPrzesylki.Typ)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Zamowienie)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Zamowienie.ZamowienieId)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index" class="btn btn-primary">Wróć do listy</a>
    <a asp-action="Edit" asp-route-id="@Model.PrzesylkaId" class="btn btn-success">Edytuj</a>
</div>
